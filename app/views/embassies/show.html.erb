<%= render 'navbar' %>
 <div class="col m6">
    <h2 class="header"><%= @embassy[0].title %></h2>
    <div class="card horizontal">
      <div class="card-image">
        <img src=<%= @src %> class="embassy-image">
      </div>
      <div class="card-stacked">
        <div class="card-content">
          	<ul class="collection">
	    <% @embassy_info.each do |item| %>
	   		<%= check_contact_data(item)%>
		<% end %>
	</ul>
        </div>
        <div class="card-action">
          <a href=<%= get_website(@embassy_info) %>>Thailand Embassy</a>
        </div>
      </div>
    </div>
  </div>
  <div class="row container">
  	<% @visa_info.each do |item| %>
  		<p> <%= item.chomp %> </p>
  		<% end %>
  </div>
